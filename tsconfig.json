{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "declaration": true,
        "target": "es6",
        "jsx": "react",
        "traceResolution": true,
        "types": []
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
      "src/**/*"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
    
}
